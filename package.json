{
  "name": "newamadeustest",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pre-cy:run": "node PREPARE_DATASET.js",
    "cy:run": " npx cypress open --browser chrome  --e2e -e allure=true",
    "clean:folders": "rm -R -f allure-report/* && rm -R -f allure-results/*",
    "allure:report": "npx allure generate allure-results --clean  -o allure-report && allure open allure-report --port 8081 --host localhost",
    "beforetest": "npm run clean:folders",
    "tests": "npm run cy:run && npm run aftertest",
    "aftertest": "npm run allure:report",
    "cy:run:headless": "cypress run --browser chrome --headless -e allure=true"
  },

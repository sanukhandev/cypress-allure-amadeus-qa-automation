    "cypress": "^13.2.0"
  },
  "dependencies": {
    "allure": "^0.0.0",
    "allure-commandline": "^2.21.0",
    "@shelex/cypress-allure-plugin": "^2.40.0",
    "moment": "^2.29.4",
    "xlsx": "^0.18.5"
  }
